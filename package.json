    "scripts": {
        "serve": "node -r ./test/requireConfig.js -r tsconfig-paths/register src/app.ts",
        "dev": "cross-env NODE_ENV=development ts-node-dev --files --trace-warnings -r tsconfig-paths/register src/app.ts",
        "debugger": "cross-env NODE_ENV=development ts-node-dev --trace-warnings --debug --inspect --files -r tsconfig-paths/register src/app.ts",
        "production": "cross-env NODE_ENV=production ts-node-dev --trace-warnings --files -r tsconfig-paths/register src/app.ts",
        "dist": "del dist && tsc && cross-env NODE_ENV=production node -r tsconfig-paths/register dist/app.js",
        "bulid": "npx webpack --mode production",
        "test": "mocha -w -r ./test/requireConfig.js -r tsconfig-paths/register test/index.ts"
    },
